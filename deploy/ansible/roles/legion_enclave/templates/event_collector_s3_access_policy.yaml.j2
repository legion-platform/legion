{
  "Version": "2012-10-17",
  "Statement": [
     {
      "Effect": "Allow",
      "Action": ["s3:*"],
      "Resource": [
        "arn:aws:s3:::{{ collector.bucket }}/{{ enclave }}",
        "arn:aws:s3:::{{ collector.bucket }}/{{ enclave }}/*"]
    },
    {
      "Effect": "Allow",
      "Action": ["s3:ListBucket"],
      "Resource": "arn:aws:s3:::{{ collector.bucket }}"
    }
  ]
}
